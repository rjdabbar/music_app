<h1>Sign up!</h1>

<form action="<%= new_user_url %>" method="post">
  <% form_auth_token %>

  <label for="user_email">Email:</label>
  <input id="user_email"
         type="textarea"
         name="user[email]"
         value="example@email.com">
  <br>
  <label for="user_password">Password</label>
  <input id="user_password"
         type="password"
         name="user[password]"
         value="">
  <br>
  <button name="Sign In">Sign up!</button>
</form>
